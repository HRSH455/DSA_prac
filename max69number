
class Solution {
    public int maximum69Number (int num) {
        char[] num1 = String.valueOf(num).toCharArray();
        for(int i=0;i<num1.length;i++){
            if(num1[i] == '6'){
                num1[i] = '9';
                break;
            }
        }
        return Integer.parseInt(new String(num1));
        
    }
}
